#include <iostream>
#include <algorithm>
#include <cstring>
#include <string>

using namespace std;

int pi[25];


int main()
{
	int n;
	cin >> n;
	for (int i = 0; i < n; i++)
		pi[i] = 1;
	int p[25];
	for (int i = 0; i < n; i++)
		cin >> p[i];

	for (int i = 1; i < n; i++){
		for (int j = 0; j < i; j++){
			if (p[j]>=p[i]){
				if (pi[j] + 1>pi[i])
					pi[i] = pi[j] + 1;
			}
		}
	}

	int max = -1;
	for (int i = 0; i < n; i++){
		if (pi[i]>max)
			max = pi[i];
	}

	cout << max;
	system("pause");

}
