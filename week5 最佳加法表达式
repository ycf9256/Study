#include <iostream>
#include <algorithm>
#include <cstring>
#include <string>

using namespace std;


int num[50][50];
int a[50];

int V(int n, int m)
{	
	long q=0xffff;
	
	if (m == 0)
		return num[1][n];
	else if (m + 1 > n)
		return 0xffff;
	else{
		for (int i = m; i < n; i++)
			q = min(q, (long)(V(i, m - 1) + num[i + 1][n]));
		return q;
	}

}

int main()
{


	int m, n;
	cin >> n >> m;
	for (int i = 1; i <= n; i++){
		cin >> a[i];
	}
	for (int i = 1; i <= n; i++){
		num[i][i] = a[i];
		for (int j = i + 1; j <= n; j++)
			num[i][j] = num[i][j - 1] * 10 + a[j];
	}
	
	cout << V(n, m);

	system("pause");

}
